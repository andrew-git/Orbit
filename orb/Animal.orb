
(print "Animal Module")

(var cat
    {
        name: "Toby"
        age: 2
        likes: ["sleep" "food"]
        color:
        {
            red: 255
            green: 128
            blue: 64
        }
        action: (function (verb noun) (print this.name verb noun))
    }
)

// define a module-level variable named kitty
// and set it's value to the cat above
(module kitty cat)